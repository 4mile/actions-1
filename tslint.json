{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
      "object-literal-sort-keys": false,
      "no-var-requires": false,
      "no-string-throw": false,
      "member-access": [true, "no-public"],
      "semicolon": [true, "never"]
    },
    "rulesDirectory": []
}
